/* groovylint-disable-next-line CompileStatic */
pipeline {
    agent 'any'
    tools {
        maven 'MAVEN3'
    }
    stages {
        stage('checkout the code') {
            steps {
                git 'https://github.com/EziamaUgonna/simple_application.git'
            }
        }
        stage('Build the code') {
            steps {
                sh 'mvn clean package'
            }
        }
        
    }
}
